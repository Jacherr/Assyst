  "compilerOptions": {
    "target": "es5",                         
    "module": "commonjs",                    
    "declaration": true,                     
    "outDir": "./JS",                         
    "downlevelIteration": true,
    "strict": true,
    "esModuleInterop": true,    
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
